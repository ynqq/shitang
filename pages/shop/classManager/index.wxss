
.hidden,.none{display: none!important;}

page {
    background-color: #fff;
}

.touchView {
    z-index: 200;
    width: 200rpx;
    position: absolute;
    top: 0;
    left: 500rpx;
    top: 0;
}

.edit_row {
    width: 100%;
    overflow: hidden;
    font-size: 24rpx;
    height: 130rpx;
    box-sizing: border-box;
    padding: 30rpx 0;
    border-bottom: 2rpx #ddd solid;
}

.edit_col1 {
    float: left;
    width: 12%;
    text-align: center;
}

.edit_col2 {
    float: left;
    width: 40%;
}

.edit_col3 {
    float: left;
    width: 24%;
    text-align: center;
}

.edit_col4 {
    float: left;
    width: 23%;
    text-align: center;
    position: relative;
}

.edit_del {
    float: left;
    padding-right: 20rpx;
    color: #fd7736;
}

.edit_finish {
    float: right;
    padding-right: 20rpx;
    width: 24%;
    height: 80rpx;
    color: #fd7736;
}

.edit_row_title .edit_col4:before {
    content: '';
    position: absolute;
    width: 2rpx;
    height: 36rpx;
    background-color: #e8e8e8;
    left: 0%;
    top: 50%;
    margin-top: -18rpx;
}

.edit_title_col1 {
    position: relative;
    left: 20rpx;
}

.stay_top {
    height: 94rpx;
    line-height: 94rpx;
}

.stay_top .edit_row {
    height: 94rpx;
    padding: 0;
    position: fixed;
    z-index: 99;
    left: 0;
    top: 0;
    width: 100%;
    color: rgba(89, 89, 89, .8);
    font-size: 28rpx;
}

.edit_row_holder {
    background-color: #fff;
}

.edit_row_on .ico_checker {
    background-position: 0 -34rpx;
}

.moveable_row {
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
}

.moveable_row .edit_row {
    box-shadow: 0 0 24rpx rgba(0, 0, 0, .2)
}

.dragSelected {
    background-color: #ccc;
}

.placeholder {
    display: inline-block;
    width: 2rpx;
    height: 2rpx;
}

.ico_delete {
    display: inline-block;
    width: 100%;
    padding-top: 100%;
    margin-top: -18%;
    background: url(data:image/png;base64,) center center no-repeat;
    -webkit-background-size: 36rpx auto;
    background-size: 36rpx auto;
}

.ico_stay_top {
    display: inline-block;
    width: 32rpx;
    height: 34rpx;
    background: url(data:image/png;base64,) 0 0 no-repeat;
    -webkit-background-size: 100% auto;
    background-size: 100% auto;
    margin-top: 16rpx;
}

.ico_drag {
    display: inline-block;
    width: 48rpx;
    height: 24rpx;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAADAAAAAYCAYAAAC8/X7cAAAAAXNSR0IArs4c6QAAAEtJREFUWAnt1cEJACAMA0B1/506moIr5FEK51+Jl0d2Vd01+JzB2X90H5jeoPwECBDIBLYlzgDj25Y4JvQAAQIERgtY4u76LHF3Aw8rGQnK3sYAXQAAAABJRU5ErkJggg==) 0 0 no-repeat;
    -webkit-background-size: 100% auto;
    background-size: 100% auto;
    margin-top: 20rpx;
}

.secname {
    font-size: 34rpx;
    line-height: 1.2;
    margin-top: 10rpx;
}

.seccode {
    font-size: 26rpx;
    color: rgba(89, 89, 89, .6);
    line-height: 1.2;
}


.stay_bottom {
    height: 100rpx;
}

.bottom_row {
    width: 100%;
    overflow: hidden;
    background-color: #fff;
    line-height: 98rpx;
    height: 98rpx;
    border-top: 2rpx #a5a5a5 solid;
    position: fixed;
    z-index: 99;
    left: 0;
    bottom: 0;
    font-size: 34rpx;
    color: #929292;
}

.stay_bottom .all_select .ico_checker {
    background-position: 0 -34rpx;
}

.add{
    height: 130rpx;
    text-align: center;
    line-height: 130rpx;
    border-bottom: 2rpx solid #ddd;
}