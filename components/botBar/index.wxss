/* components/botBar/index.wxss */
.bottomBox{
  height: 96rpx;
}
.bottomBox .bottomBar{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 96rpx;
  display: flex;
  box-shadow: 0rpx 0rpx 10rpx #ccc;
  background: #fff;
}
.bottomBox .barItem{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #8a8a8a;
}
.bottomBox .active{
  color: #1296db;
}
.bottomBox image{
  width: 32rpx;
  height: 32rpx;
  margin-bottom: 8rpx;
}